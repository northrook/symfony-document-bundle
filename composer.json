{
  "name": "northrook/symfony-document-service",
  "description": "Generates view parameters for a use in a Latte template.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Document\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2",
    "symfony/http-kernel": "@stable",
    "symfony/config": "@stable",
    "symfony/dependency-injection": "@stable",
    "northrook/core": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/support": "dev-main",
    "northrook/types": "dev-main"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "symfony/deprecation-contracts": "@stable",
    "symfony/var-dumper": "@stable",
    "symfony/error-handler": "@stable"
  },
  "replace": {
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  }
}